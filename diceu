#! /usr/bin/env ruby
require 'zlib'
require 'securerandom'
require 'base64'
require 'ostruct'

srand SecureRandom.random_bytes(4).unpack("N")[0]
module Diceudiceu
  DEFAULT_SETTINGS = {
    size: 6,
    symbols: false,
    leet: false,
    numbers: false,
    count: '1',
  }
  COLORS = %w[black red green yellow blue magenta cyan white]
    .map
    .with_index {|color, index| [color, index]}
    .to_h
  module BarrelOfMonkeys
    refine Array do
      def secure_sample
        sample(random: Random.new(SecureRandom.random_bytes(4).unpack("N")[0]))
      end
    end

    refine String do
      COLORS.compact.each do |name, num|
        define_method(name) do
          "\e[3#{num}m#{self}\e[0m"
        end
      end

      def bold
        "\e[1m#{self}\e[0m"
      end
    end
  end

  using BarrelOfMonkeys

  module ClassMethods
    def cli_exe
      parsed_args.count[/\d+/].to_i.times{ puts mk_password }
    end

    def parsed_args
      @args ||= OpenStruct.new(DEFAULT_SETTINGS.merge(
        size: ARGV.find{ |arg| arg[/^\d+$/]}.then{|num| num.nil? ? nil : num.to_i },
        symbols: ARGV.find{ |arg| arg[/^sym/] },
        numbers: ARGV.find{ |arg| arg[/^num/] },
        leet: ARGV.find{ |arg| arg[/^leet/] },
        count: ARGV.find{ |arg| arg[/^(?:ct|count)=?(\d+)/,1] }
      ) { |_,o,n| n || o } )
    end

    def mk_password
      injection_ratio = 1/3.0
      parsed_args
      injected_ct = parsed_args.size * injection_ratio
      leftover_ct = parsed_args.size - injected_ct - 1
      leetslots = [true] + ([true] * injected_ct) + ([nil] * leftover_ct)
      leetslots = leetslots.shuffle
      specslots = leetslots.dup.shuffle

      inject_leet = -> (str) do
        return str unless parsed_args.leet
        str.split('')
          .map do |letter|
            next letter unless letter.downcase[/[aeio]/]
            next letter unless leetslots.pop
            {
              'a' => 4,
              'e' => 3,
              'i' => 1,
              'o' => 0,
              'b' => 8,
              'g' => 6,
              'j' => ']',
              's' => 5,
              't' => 7,
              'u' => 'v',
              'v' => 'u',
              'z' => 2
            }[letter.downcase].to_s.green
          end
          .join
      end

      inject_specials = -> (str) do
        return str unless parsed_args.symbols
        symbols = %{!@#$%^&*()_+<>?:;"'~`}
        return str unless specslots.pop

        str + symbols.split('').secure_sample.blue
      end

      (0...parsed_args.size)
        .map{ DEFAULT_WORDLIST.secure_sample }
        .map(&:capitalize)
        .map(&inject_leet)
        .map(&inject_specials)
        .tap{ |words|
          next unless parsed_args.numbers

          3.times {words << rand(0..9).to_s.yellow}
        }
        .shuffle
        .map{|word| word.split('')}
        .flatten
        .map{|letter| letter[/[A-Z]/] ? letter.bold : letter}
        .join
    end
  end

  extend ClassMethods

  # Deflated wordlist @ ~1/2 the size!
  COMPRESSED_LIST = "".freeze
  DEFAULT_WORDLIST = Zlib::Inflate.inflate(
    Base64.decode64 COMPRESSED_LIST
  ).split("\n")
end

# if is run directly OR if it's a "bin"
if __FILE__ == $0 || !!$0[%r{/bin/diceu$}]
  Diceudiceu.cli_exe
end
